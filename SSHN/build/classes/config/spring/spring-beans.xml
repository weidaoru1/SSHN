<?xml version="1.0" encoding="UTF-8"?>  
    <beans xmlns="http://www.springframework.org/schema/beans"  
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
        xmlns:context="http://www.springframework.org/schema/context"  
        xmlns:mvc="http://www.springframework.org/schema/mvc"  
        xsi:schemaLocation="http://www.springframework.org/schema/beans   
        http://www.springframework.org/schema/beans/spring-beans.xsd">  
          
     <bean id="modelDao" class="com.fus.cocoon.dao.ModelDaoImpl">  
            <property name="sessionFactory" ref="sessionFactory"></property>  
        </bean>  
      
        <bean id="modelS" class="com.fus.cocoon.service.ModelServiceImpl">  
            <property name="modelDao" ref="modelDao"></property>  
        </bean>  
        <bean name="modelService" parent="transactionProxy">  
            <property name="target" ref="modelS"></property>  
        </bean>    
        
        
        
    </beans>  